<%= simple_form_for(@income) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :subject %>
    <%= f.input :households %>
    <%= f.input :families %>
    <%= f.input :married-couples %>
    <%= f.input :nonfamily %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
